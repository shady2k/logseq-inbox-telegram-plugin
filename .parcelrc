{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "@parcel/transformer-js"
    ]
  },
  "optimizers": {
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "@parcel/optimizer-terser"
    ]
  },
  "resolvers": ["@parcel/resolver-default"],
  "packagers": {
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": "@parcel/packager-js"
  }
}